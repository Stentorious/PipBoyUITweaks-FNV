<image name="MM_SearchButton">
	<visible> <copy src="sibling(MM_SearchBar)" trait="visible" /> </visible>
	<alpha> 255 </alpha>
	<depth> 6 </depth>
	<x>
		<copy src="sibling(MM_SearchBar)" trait="x" />
		<sub>
			<copy src="me()" trait="height" />
			<add> 6 </add>
		</sub>
	</x>
	<y>
		<copy src="sibling(MM_SearchBar)" trait="y" />
		<sub> 6 </sub>
	</y>
	<width> 32 </width>
	<height> <copy src="me()" trait="width" /> </height>
	<zoom> &scale; </zoom>
	<systemcolor> &pipboy; </systemcolor>
	<filename>
		<copy src="me()" trait="_Status" />
		<copy src="me()" trait="_Tile_" />
	</filename>
	<_Status>
		<copy> 0 </copy>
		<add>
			<copy> 1 </copy>
			<onlyif>
				<copy src="sibling(MM_SearchBar)" trait="width" />
				<gt> 10 </gt>
			</onlyif>
		</add>
	</_Status>
	<_Tile_0> lStewieAl\search.dds </_Tile_0>
	<_Tile_1> lStewieAl\searchStop.dds </_Tile_1>
</image>