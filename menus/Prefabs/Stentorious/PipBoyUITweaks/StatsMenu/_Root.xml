<!-- Colorful Crippled Limbs Meters -->
<_PipTweaks+Limb_Alpha>
	<copy src="io()" trait="_PipTweaks+Pulse" />
	<onlyif src="io()" trait="_PipTweaks+Color_Limb" />
	<add> 255 </add>
</_PipTweaks+Limb_Alpha>
<_PipTweaks+Limb_Systemcolor>
	<copy> -2 </copy>
	<onlyif src="io()" trait="_PipTweaks+Color_Limb" />
	<add> 4 </add>
</_PipTweaks+Limb_Systemcolor>

<!-- Colorful Hardcore Meters -->
<_PipTweaks+H2O_Red>
	<copy src="io()" trait="_PlrStat+H2O" />
	<div src="io()" trait="_PlrStat+H2O1" />
	<mul> 255 </mul>
	<max> 0 </max>
	<min> 255 </min>
</_PipTweaks+H2O_Red>
<_PipTweaks+H2O_Green>
	<copy> 255 </copy>
	<sub>
		<copy src="io()" trait="_PlrStat+H2O" />
		<sub src="io()" trait="_PlrStat+H2O1" />
		<div>
			<copy src="io()" trait="_PlrStat+H2O2" />
			<sub src="io()" trait="_PlrStat+H2O1" />
		</div>
		<mul> 127.5 </mul>
		<max> 0 </max>
		<min> 127.5 </min>
	</sub>
	<sub>
		<copy src="io()" trait="_PlrStat+H2O" />
		<sub src="io()" trait="_PlrStat+H2O2" />
		<div>
			<copy src="io()" trait="_PlrStat+H2O3" />
			<sub src="io()" trait="_PlrStat+H2O2" />
		</div>
		<mul> 127.5 </mul>
		<max> 0 </max>
		<min> 127.5 </min>
	</sub>
</_PipTweaks+H2O_Green>
<_PipTweaks+H2O_Alpha>
	<copy src="io()" trait="_PipTweaks+Pulse" />
	<onlyif>
		<copy src="io()" trait="_PlrStat+H2O" />
		<gte src="io()" trait="_PlrStat+H2O4" />
		<and src="io()" trait="_PipTweaks+Color_H2O" />
	</onlyif>
	<add> 255 </add>
</_PipTweaks+H2O_Alpha>
